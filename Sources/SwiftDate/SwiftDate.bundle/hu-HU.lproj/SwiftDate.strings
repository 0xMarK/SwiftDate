// RELEVANT TIME TO PRINT ALONG COLLOQUIAL STRING WHEN .includeRelevantTime = true
"relevanttime_y"                    =    "yyyy. MMM";            // for colloquial year (=+-1) adds a time string like this:"(Feb 2016)"
"relevanttime_yy"                    =    "yyyy. MMM";            // for colloquial years (>1) adds a time string like this:"(Feb 2016)"
"relevanttime_m"                    =    "yyyy. MMM dd.";        // for colloquial month (=+-1) adds a time string like this:"(Feb 17, 2016)"
"relevanttime_mm"                    =    "yyyy. MMM dd.";        // for colloquial months (>1) adds a time string like this: "(Feb 17, 2016)"
"relevanttime_w"                    =    "MMM dd., EEE";        // for colloquial months (>1) adds a time string like this: "(Wed Feb 17)"
"relevanttime_ww"                    =    "MMM dd., EEE";        // for colloquial months (>1) adds a time string like this: "(Wed Feb 17)"
"relevanttime_d"                    =    "MMM dd., EEE";        // for colloquial day (=+-1) adds a time string like this: "(Wed Feb 17)"
"relevanttime_dd"                    =    "MMM dd., EEE";        // for colloquial days (>1) adds a time string like this: "(Wed Feb 17)"
"relevanttime_h"                    =    "HH:mm'-kor'";    // for colloquial day (=+-1) adds a time string like this: "(At 13:20)"
"relevanttime_hh"                    =    "HH:mm'-kor'";    // for colloquial days (>1) adds a time string like this: "(At 13:20)"
"relevanttime_M"                    =    "";                    // for colloquial minute(s) we have not any relevant time to print
"relevanttime_MM"                    =    "";                    // for colloquial minute(s) we have not any relevant time to print
"relevanttime_s"                    =    "";                    // for colloquial seconds(s) we have not any relevant time to print
"relevanttime_ss"                    =    "";                    // for colloquial seconds(s) we have not any relevant time to print

